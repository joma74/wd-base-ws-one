	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Parcel",
			"program": "node_modules/.bin/parcel",
			"args": ["--port", "9500", "index.html"],
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node",
			"runtimeArgs": ["--nolazy"],
			"console": "integratedTerminal",
			"trace": true
		}
	]
}
